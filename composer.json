{
    "name": "smitmartijn/laravel-lookbook",
    "description": "Laravel Lookbook is a component development environment for Laravel that helps you build, document, and organize your Blade components in isolation.",
    "version": "1.0.0",
    "type": "library",
    "license": "LGPL-3.0-or-later",
    "require": {
        "php": ">=8.1",
        "laravel/framework": ">=10.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelLookbook\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Martijn Smit",
            "email": "martijn@lostdomain.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "LaravelLookbook\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelLookbook\\LookbookServiceProvider"
            ],
            "aliases": {
                "Lookbook": "LaravelLookbook\\Facades\\Lookbook"
            }
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}